               <ref name="vedaweb_model.gLike"/>
               <ref name="vedaweb_model.phrase"/>
               <ref name="vedaweb_model.inter"/>
               <ref name="vedaweb_model.global"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="vedaweb-l-abstractModel-structure-l-constraint-report-8">
            <rule context="tei:l">
               <report test="ancestor::tei:l[not(.//tei:note//tei:l[. = current()])]">
