   <define name="vedaweb_lg">
      <element name="lg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(line group) contains one or more verse lines functioning as a formal unit, e.g. a stanza, refrain, verse paragraph, etc. [3.12.1. Core Tags for Verse 3.12. Passages of Verse or Drama 7.2.5. Speech Contents]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="vedaweb_model.divTop"/>
                  <ref name="vedaweb_model.global"/>
               </choice>
            </zeroOrMore>
            <choice>
               <ref name="vedaweb_model.lLike"/>
               <ref name="vedaweb_model.stageLike"/>
               <ref name="vedaweb_model.labelLike"/>
               <ref name="vedaweb_lg"/>
            </choice>
            <zeroOrMore>
               <choice>
                  <ref name="vedaweb_model.lLike"/>
                  <ref name="vedaweb_model.stageLike"/>
                  <ref name="vedaweb_model.labelLike"/>
                  <ref name="vedaweb_model.global"/>
                  <ref name="vedaweb_lg"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <group>
                  <ref name="vedaweb_model.divBottom"/>
                  <zeroOrMore>
                     <ref name="vedaweb_model.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="vedaweb-lg-atleast1oflggapl-constraint-assert-6">
            <rule context="tei:lg">
               <sch:assert xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns="http://www.tei-c.org/ns/1.0"
                           test="count(descendant::tei:lg|descendant::tei:l|descendant::tei:gap) &gt; 0">An lg element
        must contain at least one child l, lg or gap element.</sch:assert>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="vedaweb-lg-abstractModel-structure-l-constraint-report-9">
            <rule context="tei:lg">
               <report test="ancestor::tei:l[not(.//tei:note//tei:lg[. = current()])]">
        Abstract model violation: Lines may not contain line groups.
      </report>
            </rule>
         </pattern>
         <ref name="vedaweb_att.global.attribute.xmlid"/>
         <ref name="vedaweb_att.global.attribute.n"/>
         <ref name="vedaweb_att.global.attribute.xmllang"/>
         <ref name="vedaweb_att.global.attribute.xmlbase"/>
         <ref name="vedaweb_att.global.attribute.xmlspace"/>
         <ref name="vedaweb_att.global.rendition.attribute.rend"/>
         <ref name="vedaweb_att.global.rendition.attribute.style"/>
         <ref name="vedaweb_att.global.rendition.attribute.rendition"/>
         <ref name="vedaweb_att.global.analytic.attribute.ana"/>
         <ref name="vedaweb_att.global.responsibility.attribute.cert"/>
         <ref name="vedaweb_att.global.responsibility.attribute.resp"/>
         <ref name="vedaweb_att.divLike.attributes"/>
         <ref name="vedaweb_att.typed.attributes"/>
         <ref name="vedaweb_att.declaring.attributes"/>
         <optional>
            <attribute name="source">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">VedaWeb: The attribute source is used to label the different sources of the Rigveda employed in this project.</a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>strata</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>zurich</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>vnh</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>lubotsky</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>aufrecht</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>padapatha</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>eichler</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>geldner</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>grassmann</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>otto</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>griffith</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>macdonell</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>mueller</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>oldenberg</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>renou</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                        <value>elizarenkova</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="vedaweb_teiCorpus">
      <element name="teiCorpus">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains the whole of a TEI encoded corpus, comprising a single corpus header and one or more TEI elements, each containing a single text header and a text. [4. Default Text Structure 15.1. Varieties of Composite Text]</a:documentation>
         <group>
            <ref name="vedaweb_teiHeader"/>
            <choice>
               <group>
          
                  <oneOrMore>
                     <ref name="vedaweb_model.resourceLike"/>
                  </oneOrMore>
          
          
                  <zeroOrMore>
                     <choice>
                        <ref name="vedaweb_TEI"/>
                        <ref name="vedaweb_teiCorpus"/>
                     </choice>
                  </zeroOrMore>
          
               </group>
        
               <oneOrMore>
                  <choice>
                     <ref name="vedaweb_TEI"/>
                     <ref name="vedaweb_teiCorpus"/>
                  </choice>
               </oneOrMore>
        
            </choice>
         </group>
         <ref name="vedaweb_att.global.attributes"/>
